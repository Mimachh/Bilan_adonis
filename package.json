{
  "name": "bilan",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "license": "UNLICENSED",
  "scripts": {
    "start": "node bin/server.js",
    "build": "node ace build",
    "dev": "node ace serve --hmr",
    "test": "node ace test",
    "lint": "eslint .",
    "format": "prettier --write .",
    "typecheck": "tsc --noEmit",
    "docker:compose:db:only": "docker-compose down && docker compose -f compose.yaml up -d --build",
    "docker:compose:db": "docker-compose down && docker compose -f compose.yaml up -d --build && npm run dev"
  },
  "imports": {
    "#database/*": "./database/*.js",
    "#tests/*": "./tests/*.js",
    "#boot/*": "./boot/*.js",
    "#config/*": "./config/*.js",
    "#auth/*": "./src/auth/*.js",
    "#dashboard/*": "./src/admin/dashboard/*.js",
    "#public/*": "./src/public/*.js",
    "#admin/*": "./src/admin/*.js",
    "#core/*": "./src/core/*.js"
  },
  "devDependencies": {
    "@adonisjs/assembler": "^7.7.0",
    "@adonisjs/eslint-config": "^1.3.0",
    "@adonisjs/prettier-config": "^1.3.0",
    "@adonisjs/tsconfig": "^1.3.0",
    "@japa/assert": "^1.2.4",
    "@japa/plugin-adonisjs": "^3.0.1",
    "@japa/runner": "^3.1.4",
    "@swc/core": "^1.5.24",
    "@types/luxon": "^3.4.2",
    "@types/node": "^20.13.0",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.19",
    "eslint": "^8.57.0",
    "hot-hook": "^0.2.6",
    "pino-pretty": "^11.1.0",
    "postcss": "^8.4.38",
    "prettier": "^3.3.0",
    "tailwindcss": "^3.4.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5",
    "vite": "^5.2.12"
  },
  "dependencies": {
    "@adonisjs/auth": "^9.2.1",
    "@adonisjs/core": "^6.9.1",
    "@adonisjs/cors": "^2.2.1",
    "@adonisjs/inertia": "^1.1.0",
    "@adonisjs/lucid": "^20.6.0",
    "@adonisjs/session": "^7.4.0",
    "@adonisjs/shield": "^8.1.1",
    "@adonisjs/static": "^1.1.1",
    "@adonisjs/vite": "^3.0.0",
    "@inertiajs/react": "^1.2.0",
    "@radix-ui/react-dialog": "^1.1.1",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-select": "^2.0.0",
    "@radix-ui/react-separator": "^1.0.3",
    "@radix-ui/react-slot": "^1.0.2",
    "@vinejs/vine": "^2.0.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "edge.js": "^6.0.2",
    "framer-motion": "^11.2.10",
    "lucide-react": "^0.395.0",
    "luxon": "^3.4.4",
    "next-themes": "^0.3.0",
    "pg": "^8.12.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "reflect-metadata": "^0.2.2",
    "sonner": "^1.5.0",
    "tailwind-merge": "^2.3.0",
    "tailwindcss-animate": "^1.0.7",
    "zustand": "^4.5.2"
  },
  "hotHook": {
    "boundaries": [
      "./app/controllers/**/*.ts",
      "./app/middleware/*.ts"
    ]
  },
  "eslintConfig": {
    "extends": "@adonisjs/eslint-config/app"
  },
  "prettier": "@adonisjs/prettier-config"
}
